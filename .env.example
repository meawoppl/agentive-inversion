DATABASE_URL=postgres://user:password@db.neon.tech/agentive_inversion?sslmode=require

# IMAP Email Configuration (for email-poller)
# For Gmail: use an App Password from https://myaccount.google.com/apppasswords
IMAP_SERVER=imap.gmail.com
IMAP_EMAIL=your-email@gmail.com
IMAP_PASSWORD=your-app-password
POLL_INTERVAL_SECS=300
RATE_LIMIT_SECS=60
MAX_FETCH_PER_POLL=50

# Legacy OAuth config (not needed for IMAP)
GMAIL_CLIENT_ID=your-client-id.apps.googleusercontent.com
GMAIL_CLIENT_SECRET=your-client-secret

GOOGLE_CALENDAR_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CALENDAR_CLIENT_SECRET=your-client-secret

RUST_LOG=info
